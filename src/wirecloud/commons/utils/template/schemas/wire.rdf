        <rdfs:label>Option</rdfs:label>
        <rdfs:comment>An option for platform preference</rdfs:comment>
    </rdfs:Class>
    <rdfs:Class rdf:about="http://wirecloud.conwet.fi.upm.es/ns/widget#PlatformStateProperty">

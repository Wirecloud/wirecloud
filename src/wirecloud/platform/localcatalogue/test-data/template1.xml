<?xml version="1.0" ?><Template xmlns="http://wirecloud.conwet.fi.upm.es/ns/template#">
  <!-- Widget Meta-Info -->
  <Catalog.ResourceDescription>
    <Vendor>Wirecloud</Vendor>
    <Name>test</Name>
    <Version>0.1</Version>
    <Author>admin</Author>
    <Mail>test@example.com</Mail>
    <Description>__MSG_description__</Description>
    <ImageURI>images/catalogue.png</ImageURI>
    <iPhoneImageURI>images/catalogue_iphone.png</iPhoneImageURI>
    <WikiURI>doc/index.html</WikiURI>

    <Requirements>
        <Feature name="Wirecloud" />
    </Requirements>
  </Catalog.ResourceDescription>

  <Platform.Preferences>
    <Preference default="value" description="__MSG_prefDescription__" label="__MSG_prefLabel__" name="pref" type="list">
      <Option label="__MSG_optionName__" value="1"/>
    </Preference>
  </Platform.Preferences>

  <Platform.StateProperties>
    <Property label="__MSG_propLabel__" name="prop" type="text"/>
  </Platform.StateProperties>

  <Platform.Wiring>
    <OutputEndpoint friendcode="test_friend_code" label="__MSG_eventLabel__" name="event" type="text"/>
    <InputEndpoint friendcode="test_friend_code" label="__MSG_slotLabel__" name="slot" type="text"/>
  </Platform.Wiring>

  <Platform.Link>
    <XHTML href="test.html"/>
  </Platform.Link>

  <Platform.Rendering height="24" width="10"/>

  <Translations default="en">
    <Translation lang="en">
      <msg name="description">Test Widget description</msg>
      <msg name="prefDescription">Preference description</msg>
      <msg name="prefLabel">Preference label</msg>
      <msg name="optionName">Option name</msg>
      <msg name="propLabel">Property label</msg>
      <msg name="eventLabel">Event label</msg>
      <msg name="slotLabel">Slot label</msg>
    </Translation>
    <Translation lang="es">
      <msg name="description">Descripción del Widget de pruebas</msg>
      <msg name="prefDescription">Descripción de la preferencia</msg>
      <msg name="prefLabel">Etiqueta de la preferencia</msg>
      <msg name="optionName">Nombre de la opción</msg>
      <msg name="propLabel">Etiqueta de la propiedad</msg>
      <msg name="eventLabel">Etiqueta del evento</msg>
      <msg name="slotLabel">Etiqueta de la ranura</msg>
    </Translation>
  </Translations>
</Template>
